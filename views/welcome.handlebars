<h1>Welcome {{user}} </h1>

<h4>Whats happening?</h4>
<!-- Pricing Tables -->
<div class="w3-row-padding" style="margin:0 -16px">
	<div class="w3-third w3-margin-bottom">
		<ul class="w3-ul w3-border w3-white w3-center w3-opacity w3-hover-opacity-off">
			<li class="w3-black w3-xlarge w3-padding-32">Temperature</li>
			{{#each values}}
				<li class="w3-padding-16">{{temperature}}</li>
			{{/each}}


			<li class="w3-padding-16">

				<span class="w3-opacity"> Degrees</span>
			</li>
			<li class="w3-light-grey w3-padding-24">
				<button class="w3-button w3-teal w3-padding-large w3-hover-black">Check</button>
			</li>
		</ul>
	</div>

	<div class="w3-third w3-margin-bottom">
		<ul class="w3-ul w3-border w3-white w3-center w3-opacity w3-hover-opacity-off">
			<li class="w3-teal w3-xlarge w3-padding-32">Umidity</li>
			{{#each values}}
				<li class="w3-padding-16">{{umidity}}</li>
			{{/each}}

			<li class="w3-padding-16">

				<span class="w3-opacity">Dew point is 100%</span>
			</li>
			<li class="w3-light-grey w3-padding-24">
				<button class="w3-button w3-teal w3-padding-large w3-hover-black">Check</button>
			</li>
		</ul>
	</div>

	<div class="w3-third">
		<ul class="w3-ul w3-border w3-white w3-center w3-opacity w3-hover-opacity-off">
			<li class="w3-black w3-xlarge w3-padding-32">Light</li>
			{{#each values}}
				<li class="w3-padding-16">{{light}}</li>
			{{/each}}

			<li class="w3-padding-16">

				<span class="w3-opacity">Lumen</span>
			</li>
			<li class="w3-light-grey w3-padding-24">
				<button class="w3-button w3-teal w3-padding-large w3-hover-black">Check</button>
			</li>
		</ul>
	</div>
</div>
</div>



<h1>Avarage values</h1>
<div class="container">
  <h4>Temperature</h4>
  <div class="progress">
    <div class="progress-bar" role="progressbar" aria-valuenow="70" aria-valuemin="0" aria-valuemax="100" style="width:80%">
      35%
    </div>
  </div>
</div>
<div class="container">
  <h4>Umidity</h4>
  <div class="progress">
    <div class="progress-bar" role="progressbar" aria-valuenow="70" aria-valuemin="0" aria-valuemax="10" style="width:60%">
      10%
    </div>
  </div>
</div>
<div class="container">
  <h4>Light</h4>
  <div class="progress">
    <div class="progress-bar" role="progressbar" aria-valuenow="70" aria-valuemin="0" aria-valuemax="10" style="width:60%">
      25%
    </div>
  </div>
</div>




{{#section 'jquery'}}
	<script>
		$('document').ready(function() {
			$("#login").replaceWith('<a id="logout" href="/logout" onclick="w3_close()" class="w3-bar-item w3-button w3-padding w3-text-teal"><i id="log" class="fa fa-th-large fa-fw w3-margin-right"></i>Logout</a>');
			$("#signup").hide();



		});
	</script>
{{/section}}


{{#section 'head'}}
  <style>
    html {
      text-align: center;
    }
  </style>
{{/section}}
